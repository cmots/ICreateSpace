<!--miniprogram/pages/result/result.wxml-->
<view class="weui-page" wx:if="{{database=='project'}}">
<view class="weui-cells__title">项目列表</view>
<view class="weui-cells weui-cells_after-title" wx:for="{{dataList}}">
  <navigator url="../result/single?id={{item._id}}&type={{type}}&key=id" class="weui-cell weui-cell_access" hover-class="weui-cell_active">
    <view class="weui-cell__bd">{{item.proName}}</view>
    <view class="weui-cell__ft weui-cell__ft_in-access"></view>
  </navigator>
</view>
</view>

<view class="weui-page" wx:if="{{database=='user'}}">
<view class="weui-cells__title">用户列表</view>
<view class="weui-cells weui-cells_after-title" wx:for="{{dataList}}">
  <navigator url="../result/single?id={{item._id}}&type={{type}}&key=id" class="weui-cell weui-cell_access" hover-class="weui-cell_active">
    <view class="weui-cell__bd">{{item.nickName}}</view>
    <view class="weui-cell__ft weui-cell__ft_in-access"></view>
  </navigator>
</view>
</view>

